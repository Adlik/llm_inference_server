[MASTER]
jobs=0

[MESSAGES CONTROL]
disable = fixme,
          no-member,
          no-else-return,
          too-many-arguments,
          too-few-public-methods,
          too-many-locals,
          too-many-instance-attributes,
          unnecessary-pass,
          missing-docstring,
          logging-fstring-interpolation,
          ungrouped-imports




[FORMAT]
max-line-length = 120

[BASIC]
good-names = i,
             j,
             k,
             _,
             x,
             y